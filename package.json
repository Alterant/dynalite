{
  "name": "dynalite",
  "version": "0.0.20",
  "description": "A mock implementation of Amazon's DynamoDB built on LevelDB",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require should --require longjohn --reporter spec -t $([ $REMOTE ] && echo 30s || echo 4s) -b",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --require should -t 10s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mhart/dynalite.git"
  },
  "keywords": [
    "dynamodb",
    "mock",
    "fake",
    "test",
    "aws",
    "dynamo",
    "leveldb"
  ],
  "author": "Michael Hart <michael.hart.au@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhart/dynalite/issues"
  },
  "dependencies": {
    "buffer-crc32": "~0.2.1",
    "levelup": "~0.17.0",
    "memdown": "~0.5.1",
    "level-sublevel": "~5.1.1",
    "lock": "~0.0.4",
    "lazy": "~1.0.11",
    "once": "~1.2.0",
    "async": "~0.2.9",
    "big.js": "~2.4.1",
    "level-delete-stream": "0.0.1",
    "murmurhash-js": "0.0.1"
  },
  "devDependencies": {
    "mocha": "~1.13.0",
    "should": "~2.0.2",
    "aws4": "~0.2.0",
    "longjohn": "~0.2.1",
    "istanbul": "~0.1.44"
  }
}
